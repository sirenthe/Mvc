@using EduHome.Models
@using EduHome.Areas.Admin.ViewModels.SkillViewModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
	ViewData["Title"] = "Update";
	Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

@model UpdateSkillViewModel
<div class="card">
	<div class="card-body">
		<h4 class="card-title">Slider</h4>

		<form class="forms-sample" method="post" enctype="multipart/form-data">



			<div class="form-group">
				<label for="exampleInputUsername1">Name</label>
				<input class="form-control" asp-for="Name" placeholder=" Name">
				<span asp-validation-for="Name" class="text-danger"></span>
			</div>
			<div class="form-group">
				<label for="exampleInputUsername1">Percent</label>
				<input class="form-control" asp-for="Percent" placeholder=" Percent">
				<span asp-validation-for="Percent" class="text-danger"></span>
			</div>




			<button asp-action="Update" class="btn btn-warning mr-2">Update</button>

		</form>
	</div>
</div>

